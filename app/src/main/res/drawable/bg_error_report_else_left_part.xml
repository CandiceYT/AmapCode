<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <item android:state_pressed="false">
        <layer-list>
            <item android:right="-12dp">
                <shape>
                    <solid android:color="@color/bg_b"/>
                    <stroke android:width="@dimen/route_1px" android:color="@color/font_cdg"/>
                    <corners android:topLeftRadius="13dp" android:bottomLeftRadius="13dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item android:right="-12dp">
                <shape>
                    <solid android:color="@color/color_10_off_e2"/>
                    <stroke android:width="@dimen/route_1px" android:color="@color/font_cdg"/>
                    <corners android:topLeftRadius="13dp" android:bottomLeftRadius="13dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
