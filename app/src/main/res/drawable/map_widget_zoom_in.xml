<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/c_1"/>
            <corners android:topLeftRadius="20dp" android:topRightRadius="20dp"/>
            <size android:height="40dp" android:width="38dp"/>
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="#0f000000"/>
                    <corners android:topLeftRadius="20dp" android:topRightRadius="20dp"/>
                    <size android:height="40dp" android:width="38dp"/>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
