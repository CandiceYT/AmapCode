<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <item android:state_checked="false">
        <layer-list>
            <item android:drawable="@drawable/traffic_report_toggle_bg_normal"/>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/drive_way_icon_left_normal"/>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="true">
        <layer-list>
            <item android:drawable="@drawable/traffic_report_toggle_bg_checked"/>
            <item>
                <bitmap android:gravity="center" android:src="@drawable/drive_way_icon_left_checked"/>
            </item>
        </layer-list>
    </item>
</selector>
