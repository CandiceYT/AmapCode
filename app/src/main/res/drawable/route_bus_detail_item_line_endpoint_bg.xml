<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <item android:id="@+id/background">
        <shape android:shape="oval">
            <solid android:color="#ffffff"/>
            <size android:height="13dp" android:width="13dp"/>
        </shape>
    </item>
    <item android:id="@+id/foreground">
        <shape android:shape="oval">
            <solid android:color="#399fff"/>
            <size android:height="13dp" android:width="13dp"/>
        </shape>
    </item>
</layer-list>
