<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/route_3dp"/>
            <solid android:color="@color/c_12"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/route_3dp"/>
            <solid android:color="@color/white"/>
            <stroke android:width="@dimen/route_1px" android:color="@color/c_12"/>
        </shape>
    </item>
</selector>
