<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui" android:orientation="vertical" android:background="@android:color/white" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:background="#e2e2e2" android:layout_width="match_parent" android:layout_height="0.5dp"/>
    <TextView android:textSize="17dp" android:textColor="@color/f_c_2" android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:text="@string/traffic_report_success"/>
    <TextView android:textSize="13dp" android:textColor="@color/f_c_3" android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:text="@string/traffic_report_success_desc"/>
    <com.autonavi.bundle.banner.view.DBanner android:id="@+id/banner" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/traffic_report_banner_height" android:layout_marginTop="15dp"/>
    <Button android:id="@+id/btn_ok" android:layout_marginLeft="10dp" android:layout_marginTop="10dp" android:layout_marginRight="10dp" android:layout_marginBottom="10dp" android:text="@string/Ok" style="@style/traffic_report_submit_btn"/>
</LinearLayout>
