    <ImageView android:id="@+id/card_view" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitEnd"/>
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
        <ImageView android:id="@+id/logo_view" android:layout_width="85.5dp" android:layout_height="28dp" android:layout_marginTop="14.5dp" android:layout_marginRight="12dp" android:src="@drawable/gaode_env_share_logo"/>
        <TextView android:textColor="#ffffff" android:id="@+id/txt_view" android:background="@drawable/share_txt_bg" android:paddingLeft="4dp" android:paddingRight="4dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="19.5dp" android:layout_marginRight="12dp"/>
    </LinearLayout>
</FrameLayout>
