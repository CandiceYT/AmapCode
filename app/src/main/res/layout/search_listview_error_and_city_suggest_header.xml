        <TextView android:textSize="@dimen/font_26" android:textColor="@color/gary" android:id="@+id/error_title" android:paddingBottom="8dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="17dp" android:layout_marginTop="8dp" android:text="@string/another_is_find"/>
