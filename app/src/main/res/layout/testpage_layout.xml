<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui" android:orientation="vertical" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:scrollbarStyle="outsideOverlay" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="14dp">
            <Spinner android:id="@+id/sp_account_test_page_function" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="0dp"/>
            <LinearLayout android:id="@+id/ll_account_test_page_input" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            <TextView android:textColor="#bbb" android:background="#fff" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="10dp" android:text="---------log output---------"/>
            <TextView android:textColor="#000" android:id="@+id/tv_account_test_page_log" android:background="#fff" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="10dp"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
