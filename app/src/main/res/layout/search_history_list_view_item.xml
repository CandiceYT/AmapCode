<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui" android:id="@+id/main_content_rl" android:background="@drawable/search_sugg_history_item_selector" android:paddingLeft="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <ImageView android:id="@+id/img_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="16dp" android:src="@drawable/clock_search_history" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
    <TextView android:textSize="@dimen/font_h22" android:textColor="@color/gray" android:id="@+id/distance" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
    <ImageView android:textColor="@color/gray" android:id="@+id/go_here" android:focusable="false" android:focusableInTouchMode="false" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="8dp" android:src="@drawable/search_home_item_right" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:contentDescription="@string/go_here"/>
    <RelativeLayout android:id="@+id/ll_content" android:paddingTop="13dp" android:paddingBottom="13dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginRight="20dp" android:minHeight="50dp" android:layout_toLeftOf="@+id/go_here" android:layout_toRightOf="@+id/img_view" android:layout_centerVertical="true">
        <TextView android:textSize="@dimen/font_h22" android:textColor="@color/f_c_2" android:ellipsize="end" android:id="@+id/history_name" android:background="@drawable/transparent" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:includeFontPadding="false" android:layout_alignParentLeft="true"/>
        <com.autonavi.minimap.search.view.EllipsizedHiddenTextView android:textSize="@dimen/font_26" android:textColor="@color/gray" android:ellipsize="end" android:id="@+id/text_tag" android:background="@drawable/transparent" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="6dp" android:singleLine="true" android:includeFontPadding="false" android:layout_alignParentTop="true"/>
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/tag_layout" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:layout_below="@+id/history_name" android:layout_alignParentLeft="true" android:layout_alignWithParentIfMissing="true">
            <TextView android:textSize="@dimen/font_26" android:textColor="@color/f_c_3" android:gravity="center_vertical" android:id="@+id/tvPoiTag" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:includeFontPadding="false"/>
            <View android:id="@+id/divider_point" android:background="@color/poi_address_color" android:visibility="gone" android:layout_width="5px" android:layout_height="5px" android:layout_marginLeft="5dp" android:layout_marginRight="5dp"/>
        </LinearLayout>
        <TextView android:textSize="@dimen/font_26" android:textColor="@color/f_c_3" android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/addr" android:background="@drawable/transparent" android:duplicateParentState="true" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/address_" android:singleLine="true" android:includeFontPadding="false" android:layout_toRightOf="@+id/tag_layout" android:layout_below="@+id/history_name" android:layout_alignWithParentIfMissing="true"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/super_addr" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:layout_toRightOf="@+id/tag_layout" android:layout_below="@+id/history_name" android:layout_alignWithParentIfMissing="true">
            <com.autonavi.minimap.widget.SearchListColorBlockView android:id="@+id/super_addr_color_block" android:layout_width="match_parent" android:layout_height="20dp" android:layout_marginRight="10dp"/>
        </LinearLayout>
        <RatingBar android:id="@+id/rating_bar" android:visibility="gone" android:clickable="false" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_marginRight="7dp" android:max="5" android:numStars="5" android:stepSize="0.1" android:layout_below="@+id/addr" android:layout_alignParentLeft="true" android:layout_alignWithParentIfMissing="true" style="@style/RatingBarNew"/>
        <TextView android:textSize="12dp" android:textColor="@color/f_c_2" android:id="@+id/num_review" android:visibility="gone" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:layout_toRightOf="@+id/rating_bar" android:layout_below="@+id/addr"/>
    </RelativeLayout>
    <View android:id="@+id/bottom_driver" android:background="@drawable/board_line_selector" android:layout_width="match_parent" android:layout_height="1dp" android:layout_toRightOf="@+id/img_view" android:layout_below="@+id/ll_content"/>
</RelativeLayout>
