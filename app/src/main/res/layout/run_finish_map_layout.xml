        <ImageView android:id="@+id/shareButton" android:background="@drawable/icon_c_bg_b" android:visibility="visible" android:layout_width="78dp" android:layout_height="78dp" android:layout_marginTop="-39dp" android:layout_marginRight="20dp" android:src="@drawable/icon_c38" android:layout_alignTop="@+id/run_distance_bottom_view" android:layout_alignParentRight="true"/>
