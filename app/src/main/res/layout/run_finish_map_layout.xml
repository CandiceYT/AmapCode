        <LinearLayout android:orientation="vertical" android:paddingTop="@dimen/route_6dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="44dp" android:layout_marginRight="44dp" android:layout_centerInParent="true">
            <com.autonavi.minimap.route.common.widget.RouteStrokeTextView android:textSize="@dimen/f_s_12" android:textStyle="bold" android:textColor="@color/f_c_2" android:ellipsize="end" android:id="@+id/route_title_foot_from" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1" android:drawableLeft="@drawable/route_title_circle_point_text_bg_green" android:drawablePadding="@dimen/route_6dp"/>
