<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aui="http://schemas.android.com/apk/res/com.alipay.mobile.antui">
    <ImageView android:id="@+id/iv_openlayer_item_icon" android:layout_width="40dp" android:layout_height="40dp" android:layout_marginLeft="10dp" android:layout_marginTop="17dp" android:layout_marginRight="21dp" android:src="@drawable/maplayer_list_def_pic" android:scaleType="centerCrop"/>
